{
    "compilerOptions": {
        "target": "ES2016",
        "module": "CommonJS",
        "baseUrl": "./",
        "outDir": "./dist",
        "skipLibCheck": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "paths": {
            "@/*": [
                "./src/*"
            ]
        }
    }
}